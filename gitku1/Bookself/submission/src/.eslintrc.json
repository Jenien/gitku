{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "no-undef":"off",
        "no-unused-vars":"off",
        "no-undef-init":"off",
        "no-useless-escape":"off",
        "no-prototype-builtins":"off",
        "no-async-promise-executor":"off",
        "no-constant-condition":"off",
        "no-console":"off",
        "no-irregular-whitespace":"off",
    }
}
